{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "botUpload.schema.json",
    "title": "botUpload",
    "description": "keyBotUpload json schema",
    "type": "object",
    "properties": {
      "bot_id": {
        "description": "unique identifier for the bot",
        "type": "string"
      },
      "strategy": {
        "description": "Bin size of the trades",
        "type": "string"
      },
      "apiKeyID": {
        "description": "start time of the tradeBin",
        "type": "string"
      },
      "apiKeySecret": {
        "description": "end time of the tradeBin",
        "type": "string"
      }
    },
    "required": [ "bot_id", "strategy","apiKeyID","apiKeySecret"],
    "additionalProperties": false
}
  