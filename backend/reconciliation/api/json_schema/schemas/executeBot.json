{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "ExecutBot.schema.json",
  "title": "ExecutBot",
  "description": "ExecutBot json schema",
  "type": "object",
  "properties": {
    "topic": {
      "description": "kafka topic to pull the information from",
      "type": "string"
    },
    "bot_id": {
      "description": "name of the bot to execute",
      "type": "string"
    },
    "exec_env": {
      "description": "execution environment of the bot",
      "type": "string"
    },
    "time_frame": {
      "description": "time_frame we want to look at",
      "type": "string"
    }
  },
  "required": [ "topic", "bot_id","exec_env","time_frame" ],
  "additionalProperties": false
}
