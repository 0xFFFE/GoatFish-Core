{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "botUpload.schema.json",
    "title": "botUpload",
    "description": "keyBotUpload JSON schema",
    "type": "object",
    "properties": {
        "bot_id": {
            "description": "unique id for the bot",
            "type": "string"
        },
        "strategy": {
            "description": "Strategy the bot will be using",
            "type": "string"
        },
        "api_key_id": {
            "description": "API key id for the exchange we want to execute the code for",
            "type": "string"
        },
        "api_key_secret": {
            "description": "API key secret for the exchange we want to execute the code for",
            "type": "string"
        },
        "exchange": {
            "description": "Exchange the keys belong to",
            "type": "string"
        },
        "port_number": {
            "description": "Port the bot will be hosted at",
            "type": "number"
        }
    },
    "required": [
        "bot_id",
        "strategy",
        "api_key_id",
        "api_key_secret",
        "exchange",
        "port_number"
    ],
    "additionalProperties": true
}
