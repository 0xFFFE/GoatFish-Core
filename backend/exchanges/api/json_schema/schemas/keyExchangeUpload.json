{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "keyExchangeUpload.schema.json",
    "title": "keyExchangeUpload",
    "description": "keyExchangeUpload json schema",
    "type": "object",
    "properties": {
        "api_key_id": {
            "description": "API key id for the exchange we want to execute the code for",
            "type": "string"
        },
        "api_key_secret": {
            "description": "API key secret for the exchange we want to execute the code for",
            "type": "string"
        },
        "exchange": {
            "description": "Exchange the keys belong to",
            "type": "string"
        }
    },
    "required": [
        "api_key_id",
        "api_key_secret",
        "exchange"
    ],
    "additionalProperties": false
}
